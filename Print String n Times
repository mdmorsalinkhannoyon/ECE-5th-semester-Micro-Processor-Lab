.model small
.stack 100h
.Data  
msg DB 'How many times would u like to print $'
msg1 DB 10,13,'Hello!$'
.code
main proc    
     mov ax,@data
    mov ds,ax 
    
    lea dx,msg
    mov ah,9
    int 21h
  
    
    mov ah,01
    int 21h 
    mov bl,al 
    mov ah,0
    aaa  
    mov cx,ax
      
    lea dx,msg1
   
TOP:  

    mov ah,9
    int 21h
    loop TOP
    
    
    mov ah,4ch
    int 21h
    main endp
endp main
